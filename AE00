import java.util.Scanner;

class Main {
	public static void main(String[] args)throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int N=sc.nextInt();
		int k=0;
		int sum=0;
		int M=1;
		while(N>=(M*M))
		{
			k=(N-(M*M))/M;
			sum=sum+k+1;
			M++;
		}
		System.out.println(sum);
		sc.close();
	}
}

