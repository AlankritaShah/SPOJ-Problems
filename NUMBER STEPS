import java.util.Scanner;

class Main {
	public static void main(String[] args)throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while(t-->0)
		{
			int x=sc.nextInt();
			int y=sc.nextInt();
			if(x==y || y==x-2)
			{
				if(x%2==0)
					System.out.println(y+x);
				else
					System.out.println(x+y-1);
			}
			else
				System.out.println("No Number");
		}
		sc.close();
	}
}
