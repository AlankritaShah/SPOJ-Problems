import java.util.Scanner;

class Main {
	public static void main(String[] args)throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int c=sc.nextInt();
		while(!(a==0 && b==0 && c==0))
		{
			if((b-a)==(c-b))
			{
				System.out.println("AP "+(c+b-a));
			}
			else if(b/a==c/b)
			{
				System.out.println("GP "+(c*b/a));
			}
			else
			{
				break;
			}
			a=sc.nextInt();
			b=sc.nextInt();
			c=sc.nextInt();
		}
		sc.close();
	}
}
